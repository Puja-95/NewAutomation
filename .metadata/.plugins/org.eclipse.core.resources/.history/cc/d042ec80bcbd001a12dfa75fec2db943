package array;

public class MultidimensionalArray {

	public static void main(String[] args) {
		//rows & columns
		String[][] data=new String[4][2];
		//row1
		data[0][0]="user1";
		data[0][1]= "Password1";
		
		//row2
		data[1][0]="user2";
		data[1][1]= "Password2";
		
		//row3
		data[2][0]="user3";
		data[2][1]= "Password3";
		
		data[3][0]="user3";
		data[3][1]= "Password3";
	}

}
