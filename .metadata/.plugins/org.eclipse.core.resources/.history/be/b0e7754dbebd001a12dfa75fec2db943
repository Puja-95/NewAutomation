package array;

public class MultidimensionalArray {

	public static void main(String[] args) {
		// rows & columns
		int[][] data = new int[4][2];
		// row1
		data[0][0] = 1;
		data[0][1] = 2;

		// row2
		data[1][0] = 1;
		data[1][1] = 1;

		// row3
		data[2][0] = 1;
		data[2][1] = 1;

		// row4
		data[3][0] =1;
		data[3][1] = 1;

		for (int r = 0; r <data.length; r++) {
			for (int c = 0; c < data[r].length; c++) {
				System.out.print(data[r][c] + " ");
			}
			System.out.println();
		}
	}

}
