package array;

public class MultidimensionalArray {

	public static void main(String[] args) {
		// rows & columns
		int[][][][] data = new int[10][2][10][2];
		// row1
		data[0][0][][] = 1;
		data[0][1][][]= 2;

		// row2
		data[1][0] = 1;
		data[1][1] = 1;

		// row3
		data[2][0] = 1;
		data[2][1] = 1;

		// row4
		data[3][0] =1;
		data[3][1] = 1;
		
		// row4
				data[4][0] =1;
				data[4][1] = 1;
				
				// row4
				data[5][0] =1;
				data[5][1] = 1;
				
				// row4
				data[6][0] =1;
				data[6][1] = 1;
				
				// row4
				data[7][0] =1;
				data[7][1] = 1;
				
				// row4
				data[8][0] =1;
				data[8][1] = 1;
				
				// row4
				data[9][0] =1;
				data[9][1] = 1;

		for (int r = 0; r <data.length; r++) {
			for (int c = 0; c < data[r].length; c++) {
				System.out.print(data[r][c] + " ");
			}
			System.out.println();
		}
	}

}
