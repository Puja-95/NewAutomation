package ifElseStatements;

public class IfElseClass1 {

	public static void main(String[] args) {
		int a=3;
		int b=3;
		int c= 4;
		if (a>b)
		{
			if(b>c) {
				
				System.out.println("a is greater that is- "+ a);
			}
		}
		
		if (b>c) {
			if (c>a) {
				System.out.println("b is greater that is -"+ b);
			}
		}
	}

}
