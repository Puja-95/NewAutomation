package functions;

public class ReturnType {

	public static void main(String[] args) {
		sum(20, 40);
boolean c=eligiblity(18);
	}

	public static int sum(int a, int b) {
		return a + b;
	}

	public static boolean eligiblity(int age) {
		if (age < 18) {
			return false;

		} else {
			return false;
		}
	}
}
