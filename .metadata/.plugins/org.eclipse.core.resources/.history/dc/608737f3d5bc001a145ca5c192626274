package variablesAndDatatypes;

public class variables {

	public static void main(String[] args) {
		int num=2;// int-datatype(defined by java), num is variable(defined by user), 2 is data(deined by user)
		 char H='H';// char-data type, H is variable, 'H' is data
		 String var="Puja"; //String is not a data type but it is class defined in java
		 
		 
		 boolean datatype1=true;// Boolean is data type having size of 1 Byte
		 byte datatype2=2;
		 char datatype3='M';
		 short datatype4=10;
		 int datatype5=1000000000;
		 

	}

}
