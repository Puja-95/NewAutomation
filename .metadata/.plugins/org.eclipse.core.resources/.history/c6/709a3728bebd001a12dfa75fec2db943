package array;

public class MultidimensionalArray {

	public static void main(String[] args) {
		// rows & columns
		String[][] data = new String[4][2];
		// row1
		data[0][0] = "user1";
		data[0][1] = "Password1";

		// row2
		data[1][0] = "user2";
		data[1][1] = "Password2";

		// row3
		data[2][0] = "user3";
		data[2][1] = "Password3";

		// row4
		data[3][0] = "user3";
		data[3][1] = "Password3";

		for (int r = 0; r <data.length; r++) {
			for (int c = 0; c < data[r].length; c++) {
				System.out.print(data[r][c] + " ");
			}
			System.out.println();
		}
	}

}
