package array;

public class MultidimensionalArray {

	public static void main(String[] args) {
		//rows & columns
		String[][] data=new String[4][2];
		data[0][0]="user1";
		data[0][1]= "Password1";
		
		data[0][0]="user2";
		data[0][1]= "Password2";
		
		
		data[0][0]="user3";
		data[0][1]= "Password3";
		
		data[0][0]="user3";
		data[0][1]= "Password3";
	}

}
