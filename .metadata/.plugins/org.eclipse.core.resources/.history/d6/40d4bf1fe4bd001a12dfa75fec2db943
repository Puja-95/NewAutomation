package loops;

public class Assignement6 {

	public static void main(String[] args) {

		// Create a array to store fruit name
		String[] fruitname = { "Apple", "Mango" };
		System.out.println(fruitname[0]);

		// create a array o store excel values
		int[] num = { 1, 2, 3, 4 };
		System.out.println(num[2]);
	}

}
